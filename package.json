{
  "name": "gray-swatch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint --ext .ts,.js,.vue .",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:eslint && pnpm check:ts",
    "fix:prettier": "prettier --loglevel warn --write .",
    "fix:eslint": "pnpm check:eslint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:eslint"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "color-convert": "2.0.1",
    "roboto-fontface": "0.10.0",
    "vue": "3.4.15",
    "vuetify": "3.5.2",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/color-convert": "2.0.3",
    "@types/node": "20.11.16",
    "@types/webfontloader": "1.6.38",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.21.1",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-vuetify": "2.0.1",
    "vue-tsc": "2.0.2"
  }
}
