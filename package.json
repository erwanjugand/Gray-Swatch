{
  "name": "gray-swatch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint --ext .ts,.js,.vue .",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:eslint && pnpm check:ts",
    "fix:prettier": "prettier --loglevel warn --write .",
    "fix:eslint": "pnpm check:eslint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:eslint"
  },
  "dependencies": {
    "@mdi/font": "7.2.96",
    "color-convert": "2.0.1",
    "roboto-fontface": "0.10.0",
    "vue": "3.3.4",
    "vuetify": "3.3.16",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/color-convert": "2.0.1",
    "@types/node": "18.17.17",
    "@types/webfontloader": "1.6.35",
    "@vitejs/plugin-vue": "4.3.4",
    "@vue/eslint-config-typescript": "11.0.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-vue": "9.17.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-vuetify": "1.0.2",
    "vue-tsc": "1.8.11"
  }
}
