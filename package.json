{
  "name": "gray-swatch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint --ext .ts,.js,.vue .",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:eslint && pnpm check:ts",
    "fix:prettier": "prettier --loglevel warn --write .",
    "fix:eslint": "pnpm check:eslint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:eslint"
  },
  "dependencies": {
    "@mdi/font": "7.3.67",
    "color-convert": "2.0.1",
    "roboto-fontface": "0.10.0",
    "vue": "3.3.11",
    "vuetify": "3.4.7",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/color-convert": "2.0.3",
    "@types/node": "20.10.4",
    "@types/webfontloader": "1.6.38",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.19.2",
    "prettier": "3.1.1",
    "typescript": "5.3.3",
    "vite": "5.0.8",
    "vite-plugin-vuetify": "2.0.1",
    "vue-tsc": "1.8.25"
  }
}
